#include <iostream>
#include <unordered_map>
using namespace std;

int main(){
    int n,number;
    cin>>n;
    string name;
    unordered_map<string,int>marks;
    for(int i=0;i<n;i++){
        cin>>name>>number;
        marks[name]=number;
    }
    cout<<"marks of Shruti"<<marks["Shruti"]<<endl;
        cout<<"marks of Akhilesh"<<marks["Akhilesh"]<<endl;
    }
