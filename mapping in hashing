#include <bits/stdc++.h>
#include <unordered_map>
using namespace std;

int main(){
    unordered_map<string,int>umap;
    umap["Virat"]=33;
    umap["Dhoni"]=45;
    umap["Rohit"]=78;
    // for(auto x:umap)
    // cout<<x.first<<" "<<x.second<<endl;
    //now we use iterator 
    // for(auto itr=umap.begin();itr!=umap.end();itr++)
    // cout<<itr->first<<" "<<itr->second<<endl;

string key="Shruti";
if(umap.find(key)!=umap.end())
cout<<"key found"<<endl;
else
cout<<"key not found"<<endl;
umap.insert(make_pair("Mobile",15000));
key="Virat";
umap.erase(key);
   for(auto itr=umap.begin();itr!=umap.end();itr++)
    cout<<itr->first<<" "<<itr->second<<endl;
    cout<<umap.size()<<endl;
}
